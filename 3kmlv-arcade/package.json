  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "typescript": "^5.1.3"
  },
  "private": true,
  "expo": {
    "name": "3kMLV Arcade",
    "slug": "3kmlv-arcade",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gunnchos.3kmlv.arcade",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera for game scanning and AR features.",
        "NSMicrophoneUsageDescription": "This app needs access to microphone for voice commands and game audio.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library for game screenshots and media.",
        "NSLocationWhenInUseUsageDescription": "This app needs location access for local multiplayer features.",
        "NSBluetoothPeripheralUsageDescription": "This app needs Bluetooth access for controller connectivity.",
        "NSBluetoothAlwaysUsageDescription": "This app needs Bluetooth access for controller connectivity.",
        "NSLocalNetworkUsageDescription": "This app needs local network access for multiplayer gaming.",
        "NSUserTrackingUsageDescription": "This app uses tracking for personalized gaming experiences."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.gunnchos.3kmlv.arcade",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.REQUEST_INSTALL_PACKAGES"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "windows": {
      "package": "com.gunnchos.3kmlv.arcade",
      "version": "1.0.0.0"
    },
    "plugins": [
      "expo-camera",
      "expo-media-library",
      "expo-notifications",
      "expo-sensors",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ]
    ]
  }
}
